!!System settings
!! Appearance
URxvt.font:     xft:bitstream vera sans mono:size=20:antialias=true
URxvt.boldFont: xft:bitstream vera sans mono:bold:size=20:antialias=true

!! Colorscheme

! special
*.foreground: #FFFFFF
*.background: #141c21
*.cursorColor: #FF8300

! black
*.color0: #263640
*.color8: #4a697d

! red
*.color1: #d12f2c
*.color9: #fa3935

! green
*.color2: #819400
*.color10: #a4bd00

! yellow
*.color3: #b08500
*.color11: #d9a400

! blue
*.color4: #2587cc
*.color12: #2ca2f5

! magenta
*.color5: #696ebf
*.color13: #8086e8

! cyan
*.color6: #289c93
*.color14: #33c5ba

! white
*.color7: #FFFFFF
*.color15: #FFFFFF





URxvt*scrollBar: false
URxvt*cursorUnderline: true
xft.antialias:1
xft.autohint:1
xft.hinting: 1
rxvt.termName: rxvt-unicode-256color
URxvt*saveLines: 1024
URxvt.letterSpace: 1
URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1

URxvt*transparent: true
URxvt*shading: 20

rofi.lines: 5
rofi.eh: 2
rofi.width: 100
rofi.padding: 200
rofi.fullscreen: true
rofi.bw: 0
rofi.separator-style: none
rofi.hide-scrollbar: true

!                  'background', 'border', 'separator'
rofi.color-window: argb:c82d303b, #7c8389, #1d1f21
! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: argb:031d1f21, #f3f4f5, argb:031d1f21, argb:031d1f21, #9575cd 
rofi.font: System San Francisco Display 18
rofi.show-icons: true
